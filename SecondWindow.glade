<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.22"/>
  <object class="GtkWindow" id="SecondWindow">
    <property name="default-width">500</property>
    <property name="default-height">120</property>
    <property name="window-position">center</property>
    <child>
      <object class="GtkBox" >
        <property name="orientation">vertical</property>
        <property name="spacing">10</property>
        <property name="margin-start">20</property>
        <property name="margin-end">20</property>
        <property name="margin-top">20</property>
        <property name="margin-bottom">20</property>
        <child>
            <object class="GtkLabel" >
              <property name="visible">True</property>
              <property name="can-focus">False</property>
              <property name="label" translatable="yes">Enter Connection String</property>
            </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">0</property>
                  </packing>
        </child>
        <child>
          <object class="GtkEntry" id="_txt_cn">
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="_btn_ok">
            <property name="label" translatable="yes">Aceptar</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">False</property>
            <property name="pack-type">end</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>